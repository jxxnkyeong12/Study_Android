                    android:layout_weight="1"
                    android:background="#BA1717">

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="#FF1414">

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
